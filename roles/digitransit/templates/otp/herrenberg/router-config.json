{
  "routingDefaults": {
    "walkSpeed": 1.3,
    "transferSlack": 100,
    "maxTransfers": 4,
    "waitReluctance": 0.4,
    "waitAtBeginningFactor": 0.7,
    "walkReluctance": 1.75,
    "stairsReluctance": 1.65,
    "walkBoardCost": 540,
    "carParkCarLegWeight": 2,
    "itineraryFiltering": 1.0,
    "allowKeepingRentedBicycleAtDestination": true,
    "keepingRentedBicycleAtDestinationCost": 180,
    "numItineraries": 5,
    "bikeTriangleSafetyFactor": 0.4,
    "bikeTriangleSlopeFactor": 0.3,
    "bikeTriangleTimeFactor": 0.3,
    "bikeRentalPickupTime": 180,
    "bikeRentalPickupCost": 850,
    "bannedBikeRentalNetworks": ["car-sharing", "taxi", "cargo-bike"],
    "itineraryFilters" : {
      "flexOnlyToDestination": true,
      "bikeRentalDistanceRatio": 0.75,
      "parkAndRideDurationRatio": 0.6,
      "minBikeParkingDistance": 500
    },
    "transit": {
      "searchThreadPoolSize": 4
    }
  },
  "vectorTileLayers": [
    {
      "name": "stops",
      "type": "Stop",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 600,
      "expansionFactor": 0.5
    },
    {
      "name": "stations",
      "type": "Station",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 12,
      "cacheMaxSeconds": 600,
      "expansionFactor": 0.5
    },
    {
      "name": "citybikes",
      "type": "VehicleRental",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60,
      "expansionFactor": 0.5
    },
    {
      "name": "parking",
      "type": "VehicleParking",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60,
      "expansionFactor": 0.5
    }
  ],
  "updaters": [
    {% if enable_gtfs_rt %}
    {
      "type": "stop-time-updater",
      "frequencySec": 60,
      "sourceType": "gtfs-http",
      "url": "http://gtfsr.vvs.de:7788",
      "feedId": "hbg"
    },
    {
      "type": "real-time-alerts",
      "frequencySec": 60,
      "url": "http://gtfsr.vvs.de:7788",
      "feedId": "hbg"
    },
    {
      "type": "stop-time-updater",
      "frequencySec": 60,
      "sourceType": "gtfs-http",
      "url": "{{ kvv_gtfs_rt_url }}",
      "feedId": "kvv"
    },
    {% endif %}
    {
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://backend.open-booking.eu/api/gbfs/2.2/1/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequencySec": 900,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/alf/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequencySec": 900,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/bananologen/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequencySec": 900,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/gueltstein-mobil/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequencySec": 60,
      "sourceType": "gbfs",
      "url": "https://gtfs.mfdz.de/gbfs/regiorad_stuttgart/gbfs.json",
      "allowKeepingRentedBicycleAtDestination": true
    },
    {
      "type": "bike-rental",
      "frequencySec": 60,
      "sourceType": "gbfs",
      "url": "https://platform.tier-services.io/data-sharing/tier_REUTLINGEN/gbfs",
      "headers": {
        "x-api-key": "{{ tier_reutlingen_api_key }}"
      }
    },
    {
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://{{ api_hostname }}/herrenberg/taxi/gbfs.json",
      "routeAsCar": true
    },
    {
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing/gbfs.json",
      "routeAsCar": true
    },
    {
      "type": "bike-rental",
      "frequencySec": 600,
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing-teilauto-neckar-alb/gbfs.json",
      "routeAsCar": true
    },
    {% if enable_fake_bike_box -%}
    {
      "type": "vehicle-parking",
      "sourceType": "bicycle-park-api",
      "frequencySec": 600,
      "feedId": "open-bike-box",
      "url": "https://api.dev.stadtnavi.eu/herrenberg/parking/open-bike-box.json",
      "tags": ["bike-locker"]
    },
    {% endif %}
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "frequencySec": 60,
      "feedId": "park-api",
      "url": "https://{{ api_hostname }}/herrenberg/parking/parkapi.json"
    },
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "frequencySec": 600,
      "feedId": "park-api",
      "url": "https://{{ api_hostname }}/kreis_reutlingen/parking/parkapi.json"
    }
  ]
}
